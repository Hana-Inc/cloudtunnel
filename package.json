{
  "name": "cloudtunnel",
  "version": "2.0.0",
  "description": "Seamlessly connect your local environment to the world through Cloudflare Tunnels",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "cloudtunnel": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hana-Inc/cloudtunnel.git"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "npm run build && npm run test:cli",
    "test:cli": "node dist/index.js version",
    "test-server": "node test-server/index.js",
    "prepublishOnly": "npm run build",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "cloudflare",
    "tunnel",
    "cli",
    "cloudflared",
    "development",
    "local",
    "proxy",
    "devtools",
    "networking",
    "multi-tunnel"
  ],
  "author": "Hana-Inc <info@hana-inc.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^13.1.0",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.4.3",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}